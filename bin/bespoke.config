%% -*- erlang -*-
[{webapp,
  [{http_port, 80},
   {https_port, 443}]},
 {sasl, [{sasl_error_logger, false},
         {errlog_type, all}]},
 {kernel,
  [{logger_level, info}]},
 {logger,
  [{level, info},
   {handlers, [{default, logger_std_h,
                #{config => #{type => standard_io},
                  filter_default => log}}]}]}].
